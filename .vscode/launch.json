    "version": "0.2.0",
    "configurations": [
        {
            "name": "Rust",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rust/target/debug/number_matching.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true
        },
        {
            "name": ".NET Core",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/dotnet/bin/Debug/netcoreapp2.1/dotnet.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "externalTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\dist\\index.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "preLaunchTask": "compile"
        }
    ]
}